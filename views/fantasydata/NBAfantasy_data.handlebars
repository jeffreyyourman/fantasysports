<script type="text/javascript">
$('.suggesteddivshow').hide();
$('.suggestedbutton').on('click', function() {
    $('.suggesteddiv').hide();
    $('.suggesteddivshow').show();
});
$('.closesuggested').on('click', function() {
    $(".suggesteddivshow").hide();
    $('.suggesteddiv').show();
    return false;
});
  $(function () {
    $('[data-toggle="tooltip"]').tooltip({
      'selector': '',
      'placement': 'top',
      'container':'body'
    })
  });


</script>
