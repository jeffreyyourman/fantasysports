    </div>
  </div>
</div>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <div class="table-responsive">
          <table class="table table-bordered table-hover table-striped">
            <thead>
              <tr style='font-size:10px;'>
                <th>Week</th>
                <th>Position</th>
                <th>Team</th>
                <th>Team_Name</th>
                <th>Player_Name</th>
                <th>Injury_Status</th>
                <th>Type_Of_Injury</th>
                <th>Games_Played</th>
                <th>Spread</th>
                <th>Player_Salary</th>
                <th>Rank_Vs_Pos</th>
                <th>Player_Rank</th>
                <th>OU</th>
                <th>Rating</th>
                <th>Criteria_Score</th>
                <th>FP</th>
              </tr>
            </thead>
            <tbody>
              {{#each fantasydataQB}}
                <tr style='font-size:10px;'>
                  <td>{{this.Week}}</td>
                  <td>{{this.Position}}</td>
                  <td>{{this.Team}}</td>
                  <td>{{this.Team_Name}}</td>
                  <td>{{this.Player_Name}}</td>
                  <td>{{this.Injury_Status}}</td>
                  <td>{{this.Type_Of_Injury}}</td>
                  <td>{{this.Games_Played}}</td>
                  <td>{{this.Spread}}</td>
                  <td>{{this.Player_Salary}}</td>
                  <td>{{this.Rank_Vs_Pos}}</td>
                  <td>{{this.Player_Rank}}</td>
                  <td>{{this.OU}}</td>
                  <td>{{this.Rating}}</td>
                  <td>{{this.Criteria_Score}}</td>
                  <td>{{this.FP}}</td>
                </tr>
                {{/each}}
            </tbody>
          </table>
        </div>
    </div>
  </div>
</div>





